<h1>配送先編集</h1>
<%= form_with model: @shipping_address, url: "/shipping_addresses", method: :patch do |f| %>
  <div class="form-group">
    <%= f.label :"郵便番号(ハイフンなし)" %>
    <%= f.text_field :zip_code, class: 'form-control shipping_address_zip_code' %>
  </div>
  <div class="form-group">
    <%= f.label :"住所" %>
    <%= f.text_field :address, class: 'form-control shipping_address_address' %>
  </div>

  <div class="form-group">
    <%= f.label "宛名" %>
    <%= f.text_field :name, class: 'form-control shipping_address_nam' %>
  </div>>

  <div class="form-group">
    <%= f.submit "変更内容を保存", class: 'btn btn-success' %>
  </div>
<% end %>